<footer id="site-footer">
  <section class="contact">
    <div class="container">
      <div class="logo"><img src="/assets/img/logo-mark-workmark.svg" alt="widmyer corporation logo"></div>
      <div class="info">
        {% include 'blocks/contactInfo' %}
      </div>
    </div>
  </section>
  <section class="associations">
    <div class="container">
      <p>Associations & Affiliations</p>
      {% for img in generalSettings.associations.all() %}
      {% set transform = craft.imager.transformImage(img, {
        height: 40
      })%}
        <img src="{{ transform.url }}" alt="{{ img.altText ?: img.title }}">
      {% endfor %}
    </div>
  </section>
</footer>
