{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "aelvan/imager": "v2.1.5.1",
    "craftcms/contact-form": "^2.2",
    "craftcms/redactor": "^2.3",
    "ether/seo": "^3.5",
    "rias/craft-contact-form-extensions": "1.1.1",
    "sebastianlenz/linkfield": "^1.0",
    "verbb/field-manager": "2.0.5",
    "verbb/super-table": "2.1.16"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
